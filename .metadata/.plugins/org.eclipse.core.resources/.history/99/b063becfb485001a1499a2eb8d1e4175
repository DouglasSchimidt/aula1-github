import java.util.Scanner;

public class exercicioMatriz {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		int m = sc.nextInt();
		int n = sc.nextInt();

		int mat[][] = new int[3][4];

		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {

				mat[m][n] = sc.nextInt();

			}

		}

		int x = sc.nextInt();

		for (int i = 0; i < m; i++) {
			for (int j = 0; j < n; j++) {
				
				if (mat [m][n] == x) {
					
					System.out.println("Position: " + mat [m][n] );
					
					System.out.println("Left: " + mat [m-1]);
					System.out.println("Rigt: " + mat [m+1]);
					System.out.println("Up: " + mat [m-m]);
					System.out.println("Down: " + mat [m+m]);
					
				}
				
				

			}

		}
		sc.close();
	}

}
